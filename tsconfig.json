{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"]
    },
    "types": ["react-native", "react-native-reanimated"]
  },
  "extends": "expo/tsconfig.base"
}
